          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH base AS (\n\n  WITH parsed AS (\n    SELECT th.archived_at AS time,\n           th.public_key,\n           COALESCE(\n             CASE WHEN jsonb_typeof(e.value) = 'array' THEN\n               COALESCE(\n                 NULLIF(e.value->>2,'')::double precision,\n                 NULLIF(e.value->>1,'')::double precision,\n                 NULLIF(e.value->>0,'')::double precision\n               )\n             END,\n             CASE WHEN jsonb_typeof(e.value) = 'number' THEN (e.value)::text::double precision END,\n             CASE WHEN jsonb_typeof(e.value) = 'string' THEN\n               NULLIF(regexp_replace(e.value::text, '[^0-9\\.]', '', 'g'),'')::double precision\n             END\n           ) AS load_percent\n    FROM providers.telemetry_history th\n    CROSS JOIN LATERAL jsonb_each(th.disks_load_percent) AS e(key, value)\n    WHERE $__timeFilter(th.archived_at)\n  )\n  SELECT time, public_key, AVG(load_percent) AS value\n  FROM parsed\n  GROUP BY time, public_key\n\n)\nSELECT time,\n       public_key AS metric,\n       value\nFROM base\nORDER BY time ASC, metric ASC;",
          "refId": "A",
