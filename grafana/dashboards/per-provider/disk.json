          "datasource": {
            "type": "postgres",
            "uid": "postgres_ds"
          },
          "format": "time_series",
          "rawSql": "SELECT th.archived_at AS time,\n       SUM(CASE WHEN jsonb_typeof(e.value)='array' AND jsonb_array_length(e.value)>=1 THEN (e.value->>0)::double precision ELSE 0 END) AS read_iops,\n       SUM(CASE WHEN jsonb_typeof(e.value)='array' AND jsonb_array_length(e.value)>=2 THEN (e.value->>1)::double precision ELSE 0 END) AS write_iops,\n       SUM(CASE WHEN jsonb_typeof(e.value)='array' AND jsonb_array_length(e.value)>=3 THEN (e.value->>2)::double precision ELSE 0 END) AS total_iops\nFROM providers.telemetry_history th\nLEFT JOIN LATERAL jsonb_each(th.iops) AS e(disk, value) ON TRUE\nWHERE th.public_key = ${pubkeys:singlequote} AND $__timeFilter(th.archived_at)\nGROUP BY th.archived_at\nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "Disk IOPS",
      "transparent": true,
      "type": "timeseries"
