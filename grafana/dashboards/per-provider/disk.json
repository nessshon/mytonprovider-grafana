      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres_ds"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH parsed AS (\n  SELECT\n    th.archived_at AS \"time\",\n    e.key AS disk,\n    COALESCE(\n      CASE WHEN jsonb_typeof(e.value) = 'array' THEN\n        COALESCE(\n          NULLIF(e.value->>2,'')::double precision,\n          NULLIF(e.value->>1,'')::double precision,\n          NULLIF(e.value->>0,'')::double precision\n        )\n      END,\n      CASE WHEN jsonb_typeof(e.value) = 'number' THEN (e.value)::text::double precision END,\n      CASE WHEN jsonb_typeof(e.value) = 'string' THEN\n        NULLIF(regexp_replace(e.value::text, '[^0-9\\.]', '', 'g'),'')::double precision\n      END\n    ) AS load_percent\n  FROM providers.telemetry_history th\n  CROSS JOIN LATERAL jsonb_each(th.disks_load_percent) AS e(key, value)\n  WHERE th.public_key = ${pubkeys:singlequote}\n    AND $__timeFilter(th.archived_at)\n),\nper_device AS (\n  SELECT \"time\", disk, load_percent FROM parsed\n),\noverall AS (\n  SELECT \"time\", 'ALL (avg)'::text AS disk, AVG(load_percent) AS load_percent\n  FROM parsed\n  GROUP BY \"time\"\n)\nSELECT * FROM per_device\nUNION ALL\nSELECT * FROM overall\nORDER BY \"time\", disk;",
          "refId": "A"
        }
      ],
      "title": "Disk load",
      "transparent": true,
      "type": "timeseries"
