          "datasource": {
            "type": "postgres",
            "uid": "postgres_ds"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH hist AS (\n  SELECT archived_at AS time,\n         (net_recv)[1] * 1000 AS recv,\n         (net_sent)[1] * 1000 AS sent\n  FROM providers.telemetry_history\n  WHERE public_key = ${pubkeys:singlequote} AND $__timeFilter(archived_at)\n),\ncurr AS (\n  SELECT updated_at AS time,\n         (net_recv)[1] * 1000 AS recv,\n         (net_sent)[1] * 1000 AS sent\n  FROM providers.telemetry\n  WHERE public_key = ${pubkeys:singlequote} AND $__timeFilter(updated_at)\n)\nSELECT time, recv, sent FROM hist\nUNION ALL\nSELECT time, recv, sent FROM curr\nORDER BY time;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Network throughput",
      "transparent": true,
      "type": "timeseries"
